<% @games.each do |game| %>
  <%= game.title %> 
  <%= link_to 'Show', game_path(game) %> 
  <%= link_to 'Edit', edit_game_path(game) %> 
  <%= link_to 'Destroy', game_path(game),
              method: :delete,
              data: { confirm: 'Are you sure?' } %>
  <br>
<% end %>

<%= link_to 'Games', controller: 'games' %>
<%= link_to 'New Game', new_game_path %>